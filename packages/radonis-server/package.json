{
  "name": "@microeinhundert/radonis-server",
  "version": "2.0.0",
  "description": "Server provider for Radonis",
  "type": "commonjs",
  "main": "./build/providers/RadonisProvider.js",
  "types": "./build/adonis-typings/index.d.ts",
  "exports": {
    ".": {
      "types": "./build/adonis-typings/index.d.ts",
      "require": "./build/providers/RadonisProvider.js"
    },
    "./build/commands": "./build/commands/index.js",
    "./build/commands/BuildClient": "./build/commands/BuildClient.js",
    "./build/commands/GenerateTypes": "./build/commands/GenerateTypes.js",
    "./*": "./*"
  },
  "sideEffects": false,
  "files": [
    "build/adonis-typings",
    "build/commands",
    "build/templates",
    "build/providers",
    "build/src",
    "build/instructions.js",
    "build/instructions.js.map",
    "build/instructions.md",
    "build/exceptions.json"
  ],
  "scripts": {
    "clean": "del-cli build",
    "copyfiles": "copyfiles \"templates/**/*.txt\" \"instructions.md\" build",
    "build:cjs": "tsc --outDir ./build --module commonjs",
    "build": "\"$npm_execpath\" run build:cjs && \"$npm_execpath\" run copyfiles",
    "lint": "TIMING=1 eslint . --ext=.ts",
    "format": "prettier --write .",
    "test": "node -r @adonisjs/require-ts/build/register bin/test.ts"
  },
  "repository": {
    "type": "git",
    "url": "git+ssh://git@github.com/microeinhundert/radonis.git",
    "directory": "packages/radonis-server"
  },
  "bugs": {
    "url": "https://github.com/microeinhundert/radonis/issues"
  },
  "publishConfig": {
    "access": "public"
  },
  "engines": {
    "node": ">= 16.0.0",
    "npm": ">= 8.0.0",
    "pnpm": ">= 7.0.0"
  },
  "keywords": [
    "adonisjs",
    "adonis",
    "radonis",
    "server"
  ],
  "author": "Leon Seipp <l.seipp@microeinhundert.com>",
  "license": "MIT",
  "adonisjs": {
    "instructions": "./build/instructions.js",
    "instructionsMd": "./build/instructions.md",
    "types": "@microeinhundert/radonis-server",
    "commands": [
      "@microeinhundert/radonis-server/build/commands"
    ],
    "providers": [
      "@microeinhundert/radonis-server"
    ],
    "templates": {
      "basePath": "./build/templates",
      "config": [
        {
          "src": "config.txt",
          "dest": "radonis"
        }
      ],
      "contracts": [
        {
          "src": "contract.txt",
          "dest": "radonis"
        }
      ]
    }
  },
  "peerDependencies": {
    "@adonisjs/ace": "^11.3.1",
    "@adonisjs/core": "^5.8.0",
    "@adonisjs/i18n": "^1.5.0",
    "@adonisjs/session": "^6.4.0",
    "@adonisjs/sink": "^5.3.2",
    "react": "^18.2.0",
    "react-dom": "^18.2.0"
  },
  "devDependencies": {
    "@types/node": "^18.8.5",
    "@types/react": "^18.0.21",
    "@types/react-dom": "^18.0.6"
  },
  "dependencies": {
    "@microeinhundert/radonis-build": "workspace:2.0.0",
    "@microeinhundert/radonis-hooks": "workspace:2.0.0",
    "@microeinhundert/radonis-hydrate": "workspace:2.0.0",
    "@microeinhundert/radonis-shared": "workspace:2.0.0",
    "@microeinhundert/radonis-types": "workspace:2.0.0",
    "chokidar": "^3.5.3",
    "flattie": "^1.1.0",
    "minipass": "^3.3.4",
    "node-fetch": "^3.2.10",
    "superjson": "^1.10.1",
    "tslib": "^2.4.0"
  }
}
