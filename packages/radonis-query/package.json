{
  "name": "@microeinhundert/radonis-query",
  "version": "1.9.0",
  "description": "Query plugin for Radonis",
  "type": "commonjs",
  "main": "./build/cjs/src/index.js",
  "types": "./build/cjs/src/index.d.ts",
  "exports": {
    ".": {
      "import": {
        "types": "./build/esm/src/index.d.ts",
        "default": "./build/esm/src/index.js"
      },
      "require": {
        "types": "./build/cjs/src/index.d.ts",
        "default": "./build/cjs/src/index.js"
      }
    }
  },
  "sideEffects": false,
  "files": [
    "build/cjs",
    "build/esm"
  ],
  "scripts": {
    "clean": "del-cli build",
    "build:cjs": "tsc --outDir ./build/cjs --module commonjs",
    "build:esm": "tsc --outDir ./build/esm --module esnext",
    "build": "\"$npm_execpath\" run build:cjs && \"$npm_execpath\" run build:esm",
    "lint": "TIMING=1 eslint . --ext=.ts,.tsx",
    "format": "prettier --write .",
    "test": "node -r @adonisjs/require-ts/build/register bin/test.ts"
  },
  "repository": {
    "type": "git",
    "url": "git+ssh://git@github.com/microeinhundert/radonis.git"
  },
  "bugs": {
    "url": "https://github.com/microeinhundert/radonis/issues"
  },
  "publishConfig": {
    "access": "public"
  },
  "engines": {
    "node": ">= 16.0.0",
    "npm": ">= 8.0.0",
    "yarn": ">= 2.0.0",
    "pnpm": ">= 7.0.0"
  },
  "keywords": [
    "adonisjs",
    "adonis",
    "radonis",
    "plugin",
    "query"
  ],
  "author": "Leon Seipp <l.seipp@microeinhundert.com>",
  "license": "MIT",
  "peerDependencies": {
    "@microeinhundert/radonis": "1.9.0"
  },
  "devDependencies": {
    "@microeinhundert/radonis": "workspace:1.9.0",
    "@types/node": "^16.11.56",
    "@types/react": "^18.0.20",
    "@types/react-dom": "^18.0.6"
  },
  "dependencies": {
    "@microeinhundert/radonis-shared": "workspace:1.9.0",
    "@microeinhundert/radonis-types": "workspace:1.9.0",
    "@microeinhundert/radonis-hooks": "workspace:1.9.0",
    "@tanstack/react-query": "^4.3.4",
    "superjson": "^1.9.1",
    "tslib": "^2.4.0"
  }
}
