{
  "name": "@microeinhundert/radonis-server",
  "version": "0.6.4",
  "description": "Server provider for Radonis",
  "type": "commonjs",
  "main": "./build/cjs/providers/RadonisProvider.js",
  "types": "./build/cjs/adonis-typings/index.d.ts",
  "exports": {
    ".": {
      "import": {
        "types": "./build/esm/adonis-typings/index.d.ts",
        "default": "./build/esm/providers/RadonisProvider.js"
      },
      "require": {
        "types": "./build/cjs/adonis-typings/index.d.ts",
        "default": "./build/cjs/providers/RadonisProvider.js"
      }
    }
  },
  "sideEffects": false,
  "files": [
    "build/cjs",
    "build/esm",
    "build/templates",
    "build/instructions.md"
  ],
  "scripts": {
    "clean": "del build",
    "copyfiles": "copyfiles \"instructions.md\" \"templates/*.txt\" build",
    "build:cjs": "tsc --outDir ./build/cjs --module commonjs",
    "build:esm": "tsc --outDir ./build/esm --module esnext",
    "build": "\"$npm_execpath\" run clean && \"$npm_execpath\" run build:cjs && \"$npm_execpath\" run build:esm && \"$npm_execpath\" run copyfiles",
    "lint": "TIMING=1 eslint .",
    "format": "prettier --write ."
  },
  "repository": {
    "type": "git",
    "url": "git+ssh://git@github.com/microeinhundert/radonis.git"
  },
  "bugs": {
    "url": "https://github.com/microeinhundert/radonis/issues"
  },
  "publishConfig": {
    "access": "public"
  },
  "engines": {
    "node": ">= 16.0.0",
    "npm": ">= 8.0.0",
    "yarn": ">= 2.0.0"
  },
  "keywords": [
    "adonisjs",
    "adonis",
    "radonis",
    "server"
  ],
  "author": "Leon Seipp <l.seipp@microeinhundert.com>",
  "license": "MIT",
  "adonisjs": {
    "instructionsMd": "./build/instructions.md",
    "types": "@microeinhundert/radonis-server",
    "providers": [
      "@microeinhundert/radonis-server"
    ],
    "templates": {
      "basePath": "./build/templates",
      "config": [
        {
          "src": "config.txt",
          "dest": "radonis"
        }
      ]
    }
  },
  "peerDependencies": {
    "@adonisjs/core": "^5.7.0",
    "@adonisjs/i18n": "^1.5.0",
    "@adonisjs/session": "^6.2.0",
    "@adonisjs/shield": "^7.0.0",
    "react": "^18.0.0",
    "react-dom": "^18.0.0"
  },
  "devDependencies": {
    "@types/node": "^16.11.27",
    "@types/react": "^18.0.6",
    "@types/react-dom": "^18.0.2"
  },
  "dependencies": {
    "@microeinhundert/radonis-hydrate": "0.6.4",
    "@microeinhundert/radonis-manifest": "0.6.4",
    "@microeinhundert/radonis-twind": "0.6.4",
    "@poppinss/utils": "^4.0.4",
    "del": "^6.0.0",
    "esbuild": "^0.14.38",
    "flattie": "^1.1.0"
  }
}
