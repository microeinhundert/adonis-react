{
  "$schema": "https://turborepo.org/schema.json",
  "pipeline": {
    "build": {
      "outputs": ["build"],
      "dependsOn": ["clean", "^build"]
    },
    "lint": {
      "outputs": [],
      "dependsOn": ["^lint"]
    },
    "clean": {
      "outputs": [],
      "dependsOn": ["lint", "^clean"]
    },
    "format": {
      "outputs": []
    },
    "test": {
      "outputs": []
    },
    "publish": {
      "cache": false
    },
    "dev": {
      "cache": false
    }
  }
}
