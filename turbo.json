{
  "$schema": "https://turborepo.org/schema.json",
  "pipeline": {
    "build": {
      "outputs": ["build"],
      "inputs": ["src"],
      "dependsOn": ["^lint", "^clean", "^build"]
    },
    "lint": {
      "outputs": []
    },
    "clean": {
      "outputs": []
    },
    "format": {
      "outputs": []
    },
    "test": {
      "outputs": []
    },
    "dev": {
      "cache": false
    }
  }
}
