{
  "name": "radonis-example-app",
  "version": "1.4.2",
  "private": true,
  "scripts": {
    "dev": "concurrently 'npm:dev:*'",
    "dev:server": "node ace serve --watch",
    "dev:client": "node ace build:client --types-output-dir './tmp/types' --watch-dir './resources/!(views)/**/*.ts(x)?'",
    "build": "concurrently 'npm:build:*'",
    "build:server": "node ace build --production",
    "build:client": "node ace build:client --production --output-dir tsconfig-out-dir",
    "test": "node ace test",
    "start": "node server.js",
    "lint": "eslint . --ext=.ts",
    "format": "prettier --write ."
  },
  "devDependencies": {
    "@adonisjs/assembler": "^5.8.0",
    "@japa/preset-adonis": "^1.1.0",
    "@japa/runner": "^2.0.9",
    "@types/react": "^18.0.14",
    "@types/react-dom": "^18.0.5",
    "openapi-types": "^12.0.0",
    "pino-pretty": "^8.0.0",
    "youch": "^3.2.0",
    "youch-terminal": "^2.1.4",
    "concurrently": "^7.2.2"
  },
  "dependencies": {
    "@adonisjs/core": "^5.8.3",
    "@adonisjs/repl": "^3.1.11",
    "@adonisjs/session": "^6.4.0",
    "@adonisjs/shield": "^7.0.11",
    "@microeinhundert/radonis": "^1.4.2",
    "@microeinhundert/radonis-server": "^1.4.2",
    "@microeinhundert/radonis-twind": "^1.4.2",
    "@microeinhundert/radonis-unocss": "^1.4.2",
    "proxy-addr": "^2.0.7",
    "react": "^18.2.0",
    "react-dom": "^18.2.0",
    "reflect-metadata": "^0.1.13",
    "source-map-support": "^0.5.21"
  }
}
