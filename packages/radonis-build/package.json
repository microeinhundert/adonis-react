{
  "name": "@microeinhundert/radonis-build",
  "version": "1.8.0",
  "description": "Build package for Radonis",
  "type": "commonjs",
  "main": "./build/cjs/src/index.js",
  "types": "./build/cjs/src/index.d.ts",
  "exports": {
    ".": {
      "import": {
        "types": "./build/esm/src/index.d.ts",
        "default": "./build/esm/src/index.js"
      },
      "require": {
        "types": "./build/cjs/src/index.d.ts",
        "default": "./build/cjs/src/index.js"
      }
    }
  },
  "sideEffects": false,
  "files": [
    "build/cjs",
    "build/esm"
  ],
  "scripts": {
    "clean": "del-cli build",
    "build:cjs": "tsc --outDir ./build/cjs --module commonjs",
    "build:esm": "tsc --outDir ./build/esm --module esnext",
    "build": "\"$npm_execpath\" run build:cjs && \"$npm_execpath\" run build:esm",
    "lint": "TIMING=1 eslint . --ext=.ts,tsx",
    "format": "prettier --write .",
    "test": "node -r @adonisjs/require-ts/build/register bin/test.ts"
  },
  "repository": {
    "type": "git",
    "url": "git+ssh://git@github.com/microeinhundert/radonis.git"
  },
  "bugs": {
    "url": "https://github.com/microeinhundert/radonis/issues"
  },
  "publishConfig": {
    "access": "public"
  },
  "engines": {
    "node": ">= 16.0.0",
    "npm": ">= 8.0.0",
    "yarn": ">= 2.0.0",
    "pnpm": ">= 7.0.0"
  },
  "keywords": [
    "adonisjs",
    "adonis",
    "radonis",
    "build"
  ],
  "author": "Leon Seipp <l.seipp@microeinhundert.com>",
  "license": "MIT",
  "devDependencies": {
    "@types/node": "^16.11.27"
  },
  "dependencies": {
    "@microeinhundert/radonis-shared": "1.8.0",
    "@microeinhundert/radonis-types": "1.8.0",
    "@poppinss/utils": "^5.0.0",
    "esbuild": "^0.14.51",
    "fs-extra": "^10.1.0",
    "tslib": "^2.4.0"
  }
}
