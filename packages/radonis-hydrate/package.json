{
  "name": "@microeinhundert/radonis-hydrate",
  "version": "1.1.7",
  "description": "Hydration package for Radonis",
  "type": "commonjs",
  "main": "./build/cjs/src/index.js",
  "types": "./build/cjs/src/index.d.ts",
  "exports": {
    ".": {
      "import": {
        "types": "./build/esm/src/index.d.ts",
        "default": "./build/esm/src/index.js"
      },
      "require": {
        "types": "./build/cjs/src/index.d.ts",
        "default": "./build/cjs/src/index.js"
      }
    }
  },
  "sideEffects": false,
  "files": [
    "build/cjs",
    "build/esm"
  ],
  "scripts": {
    "clean": "del-cli build",
    "compile:cjs": "tsc --outDir ./build/cjs --module commonjs",
    "compile:esm": "tsc --outDir ./build/esm --module esnext",
    "compile": "\"$npm_execpath\" run lint && \"$npm_execpath\" run clean && \"$npm_execpath\" run compile:cjs && \"$npm_execpath\" run compile:esm",
    "build": "\"$npm_execpath\" run compile",
    "lint": "TIMING=1 eslint . --ext=.ts",
    "format": "prettier --write ."
  },
  "repository": {
    "type": "git",
    "url": "git+ssh://git@github.com/microeinhundert/radonis.git"
  },
  "bugs": {
    "url": "https://github.com/microeinhundert/radonis/issues"
  },
  "publishConfig": {
    "access": "public"
  },
  "engines": {
    "node": ">= 16.0.0",
    "npm": ">= 8.0.0",
    "yarn": ">= 2.0.0"
  },
  "keywords": [
    "adonisjs",
    "adonis",
    "radonis",
    "hydrate"
  ],
  "author": "Leon Seipp <l.seipp@microeinhundert.com>",
  "license": "MIT",
  "peerDependencies": {
    "react": "^18.0.0",
    "react-dom": "^18.0.0"
  },
  "devDependencies": {
    "@types/node": "^16.11.27",
    "@types/react": "^18.0.8",
    "@types/react-dom": "^18.0.2"
  },
  "dependencies": {
    "@microeinhundert/radonis-manifest": "1.1.7",
    "@microeinhundert/radonis-shared": "1.1.7"
  }
}
