{
  "trailingComma": "es5",
  "semi": false,
  "singleQuote": true,
  "useTabs": false,
  "quoteProps": "consistent",
  "bracketSpacing": true,
  "arrowParens": "always",
  "printWidth": 120,
  "overrides": [
    {
      "files": "*.json",
      "options": {
        "printWidth": 40
      }
    },
    {
      "files": "*Exception.ts",
      "options": {
        "printWidth": 60
      }
    }
  ]
}
