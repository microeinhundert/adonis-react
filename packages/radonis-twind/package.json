  },
  "engines": {
    "node": ">= 16.0.0",
    "npm": ">= 8.0.0",
    "yarn": ">= 2.0.0"
  },
  "keywords": [
    "adonisjs",
    "adonis",
    "radonis",
    "twind"
  ],
  "author": "Leon Seipp <l.seipp@microeinhundert.com>",
  "license": "MIT",
  "peerDependencies": {
    "react": "^18.0.0",
    "react-dom": "^18.0.0"
  },
  "devDependencies": {
    "@types/node": "^16.11.27",
    "@types/react": "^18.0.5",
    "@types/react-dom": "^18.0.1"
  },
  "dependencies": {
    "@twind/preset-autoprefix": "1.0.0-next.37",
    "@twind/preset-tailwind": "1.0.0-next.37",
    "@twind/preset-tailwind-forms": "1.0.0-next.37",
    "twind": "1.0.0-next.37"
  }
